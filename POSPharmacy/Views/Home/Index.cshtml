@section Scripts{

    <script type="text/javascript">
        function updateSubtotal(element) {
            const row = element.closest("tr");
            const price = 300; // Fixed price
            const qty = parseInt(row.querySelector("input[type='text']").value, 10) || 0;
            const discount = parseFloat(row.querySelector("input[type='text'][value]").value) || 0;

            // Calculate subtotal
            const subtotal = qty * price * ((100 - discount) / 100);
            row.querySelector(".subtotal").textContent = subtotal.toFixed(2);
        }

        function increaseQty(button) {
            const row = button.closest("tr");
            const qtyInput = row.querySelector("input[type='text']");
            let qty = parseInt(qtyInput.value, 10) || 0;
            qty++;
            qtyInput.value = qty;

            // Update subtotal
            updateSubtotal(qtyInput);
        }

        function decreaseQty(button) {
            const row = button.closest("tr");
            const qtyInput = row.querySelector("input[type='text']");
            let qty = parseInt(qtyInput.value, 10) || 0;

            if (qty > 0) qty--;
            qtyInput.value = qty;

            // Update subtotal
            updateSubtotal(qtyInput);
        }

        function deleteRow(button) {
            const row = button.closest("tr");
            row.remove();
        }





